        "num1=float(input(\"enter 1st no \"))\n",
        "operator=input(\"enter an operator \")\n",
        "num2=float(input(\"enter 2nd no \"))\n",
        "if operator==\"+\":\n",
        "           result=num1+num2\n",
        "elif operator==\"-\":\n",
        "           result=num1-num2\n",
        "elif operator ==\"*\":\n",
        "           result=num1*num2\n",
        "elif operator==\"/\":\n",
        "           if num2==0:\n",
        "            result=\"division by zero is not allowed\"\n",
        "           else:\n",
        "            result=num1/num2\n",
        "else:\n",
        "           result=\"invalid\"\n",
        "print(f\"{num1}{operator}{num2}={result}\")\n"
      ],
